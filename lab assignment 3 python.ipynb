   "source": [
    "###   Python program to count the numer of strings where string length is 2 or more and the 1st and last character are same from given list of strings. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3\n"
     ]
    }
   ],
   "source": [
    "list=[\"aditya\",\"guard\",\"Ram\",\"1221\",\"mam\"]\n",
    "ctr=0\n",
    "for x in list: \n",
    "    if len(x) > 1 and x[0]==x[-1]:\n",
    "        ctr += 1\n",
    "print(ctr)"
   ]
